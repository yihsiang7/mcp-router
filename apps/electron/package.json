{
  "name": "@mcp_router/electron",
  "productName": "MCP Router",
  "version": "0.6.2",
  "private": true,
  "main": ".webpack/main",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcp-router/mcp-router.git"
  },
  "scripts": {
    "start": "electron-forge start",
    "dev": "electron-forge start --enable-logging",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "package:x64": "cross-env NODE_ENV=production npm_config_target_arch=x64 electron-forge package",
    "package:arm64": "cross-env NODE_ENV=production npm_config_target_arch=arm64 electron-forge package",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "make:x64": "cross-env NODE_ENV=production npm_config_target_arch=x64 npm_config_target_platform=darwin npm_config_arch=x64 electron-forge make --arch=x64",
    "make:arm64": "cross-env NODE_ENV=production npm_config_target_arch=arm64 npm_config_target_platform=darwin npm_config_arch=arm64 electron-forge make --arch=arm64",
    "publish": "npx @electron/rebuild -f -w better-sqlite3 && cross-env NODE_ENV=production electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --ext .ts,.tsx . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "knip": "cd ../.. && knip --workspace apps/electron",
    "postinstall": "electron-rebuild",
    "rebuild": "electron-rebuild",
    "rebuild:x64": "cross-env npm_config_target_arch=x64 npm_config_arch=x64 electron-rebuild --arch=x64",
    "rebuild:arm64": "cross-env npm_config_target_arch=arm64 npm_config_arch=arm64 electron-rebuild --arch=arm64",
    "test:e2e": "pnpm make && node -e \"const os=require('os');const arch=os.arch()==='x64'?'x64':(os.arch()==='arm64'?'arm64':'');if(!arch){process.exit(1)};console.log('Using arch',arch);\" && node -e \"const fs=require('fs');const os=require('os');const arch=os.arch()==='x64'?'x64':(os.arch()==='arm64'?'arm64':'');fs.cpSync('package.json', `.webpack/${arch}/main/`);\" && playwright test --config=e2e/playwright-electron.config.ts",
    "test:e2e:only": "playwright test --config=e2e/playwright-electron.config.ts",
    "test:e2e:headed": "pnpm make && node -e \"const os=require('os');const arch=os.arch()==='x64'?'x64':(os.arch()==='arm64'?'arm64':'');if(!arch){process.exit(1)};console.log('Using arch',arch);\" && node -e \"const fs=require('fs');const os=require('os');const arch=os.arch()==='x64'?'x64':(os.arch()==='arm64'?'arm64':'');fs.cpSync('package.json', `.webpack/${arch}/main/`);\" && playwright test --config=e2e/playwright-electron.config.ts --headed"
  },
  "dependencies": {
    "@anthropic-ai/dxt": "^0.2.6",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.8",
    "@mcp_router/remote-api-types": "workspace:*",
    "@mcp_router/shared": "workspace:*",
    "@mcp_router/tailwind-config": "workspace:*",
    "@mcp_router/ui": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@tabler/icons-react": "^3.35.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@uiw/react-codemirror": "^4.25.3",
    "@xyflow/react": "^12.9.3",
    "argon2": "^0.41.1",
    "better-sqlite3": "^11.10.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "electron-squirrel-startup": "^1.0.1",
    "execa": "^9.6.1",
    "express": "^5.2.1",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "immer": "^10.2.0",
    "lucide-react": "^0.487.0",
    "node-machine-id": "^1.1.12",
    "posthog-js": "^1.299.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^15.7.4",
    "react-router-dom": "^7.9.6",
    "semver": "^7.7.3",
    "smol-toml": "^1.5.2",
    "sonner": "^2.0.7",
    "strip-ansi": "^7.1.2",
    "tailwind-merge": "^2.6.0",
    "update-electron-app": "^3.1.1",
    "uuid": "^11.1.0",
    "zustand": "^5.0.9",
    "recharts": "^2.15.3",
    "react-wordcloud": "^1.2.7",
    "d3-cloud": "^1.2.7"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/plugin-webpack": "^7.10.2",
    "@electron-forge/publisher-github": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^3.7.2",
    "@playwright/test": "^1.57.0",
    "@types/cors": "^2.8.19",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/express": "^5.0.6",
    "@types/semver": "^7.7.1",
    "@types/uuid": "^10.0.0",
    "@types/webpack": "^5.28.5",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "autoprefixer": "^10.4.22",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "dotenv": "^16.6.1",
    "electron": "^36.9.5",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "html-webpack-plugin": "^5.6.5",
    "node-loader": "^2.1.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.18",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "@types/d3-cloud": "^1.2.9",
    "webpack": "^5.103.0"
  }
}
